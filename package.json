    "pdfjs-dist": "^4.6.82",
    "react": "^18.3.1",
    "react-dom": "^18",
    "react-pdf": "^9.1.0",
    "react-youtube": "^10.1.0",
    "simple-git": "^3.27.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@jest/globals": "^29.7.0",
